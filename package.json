{
  "name": "hiera-editor",
  "version": "0.1.0",
  "description": "Hiera editor",
  "main": "dist/main.js",
  "scripts": {
    "start": "electron .",
    "debug:win32": "electron %NODE_DEBUG_OPTION% .",
    "debug:osx": "electron $NODE_DEBUG_OPTION .",
    "postinstall": "npmpd && run-script-os",
    "install:darwin": "BUNDLE_GEMFILE=./ruby/Gemfile ./node_modules/traveling-ruby-osx/bin/bundle install",
    "install:win32": "cmd /C \"set BUNDLE_GEMFILE=ruby\\Gemfile && node_modules\\traveling-ruby-win32\\bin\\gem install --bindir node_modules\\traveling-ruby-win32\\bin.real --no-ri --no-rdoc bundler && node_modules\\traveling-ruby-win32\\bin\\bundle install\""
  },
  "repository": "https://github.com/desertkun/hiera-editor",
  "keywords": [
    "Electron",
    "Puppet",
    "Hiera"
  ],
  "author": "desertkun",
  "license": "MIT",
  "devDependencies": {
    "electron": "^3.0.0",
    "npm-platform-dependencies": "0.0.13",
    "run-script-os": "^1.0.3"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.3.1",
    "app-root-path": "^2.0.1",
    "electron-ipc-tunnel": "^3.1.0",
    "electron-json-storage": "^4.1.3",
    "electron-window-state": "^4.1.1",
    "install": "^0.10.2",
    "jquery": "^3.2.1",
    "npm": "^5.6.0",
    "text-ellipsis": "^1.0.3",
    "traveling-ruby-win32": "^2.2.1"
  },
  "darwinDependencies": {
    "traveling-ruby-osx": "latest"
  },
  "win32Dependencies": {
    "traveling-ruby-win32": "latest"
  },
  "linuxDependencies": {
    "traveling-ruby-linux-x86_64": "latest"
  }
}
